package example1;

public class FindmaxUsingRecursion {

	public static void main(String[] args) {
		
		int [] num= {23,45,64,14,37,48};
		System.out.println(findMax(num));
	}

	private static int findMax(int[] num) {
		
		if(num.length < 2)
			return num[num.length-1];
		
		int max=num[0];
		
		
		return 0;
	}
}
