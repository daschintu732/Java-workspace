package example1;

public class PrimeNumber {

	public static void main(String[] args) {
		
		int num=19;
		boolean ans=checkPrime(num);
		System.out.println(ans);
	}

	public static boolean checkPrime(int num) {
		
		int count = 0;
		
		if(num<2)
			return true;
		
		for (int i = 1; i < 4; i++) {
			
			if(num%i == 0)
				count++;
		}
		
		if(count>2)
			return true;
		
		
		return false;
	}

}
